{
    "name": "starterteam/starter-freya",
    "type": "typo3-cms-extension",
    "description": "Theme extension for EXT:starter and EXT:starter-twig",
    "license": "proprietary",
    "authors": [
        {
            "name": "Christian Wolfram",
            "email": "c.wolfram@chriwo.de"
        },
        {
            "name": "Marc Fell",
            "email": "marc@fell.hamburg"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/starterteam/team/starter-freya/-/issues"
    },
    "require": {
		"php": "^8.3",
		"typo3/cms-core": "^12.4 || ^13.0",
		"starterteam/starter-twig": "^4.5 || dev-main"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.5-dev"
        },
        "typo3/cms": {
            "extension-key": "starter_freya",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
