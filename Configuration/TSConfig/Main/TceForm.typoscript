TCEFORM {
  pages {

  }

  tt_content {
    # TYPO3-Core CE's
    CType.keepItems := addToList(text)
    CType.keepItems := addToList(starter_textmedia)

    # EXT:starter CE's
    CType.keepItems := addToList(starter_media)
    CType.keepItems := addToList(starter_accordion)
    CType.keepItems := addToList(starter_distribution_navigation)
    CType.keepItems := addToList(starter_download)

    # disable TYPO3-Core fields
    image_zoom.disabled = 1

    imagecols {
      types {
        starter_textmedia.disabled = 1
      }
    }

    # disable EXT:starter fields
    tx_starter_imagecols_medium {
      types {
        starter_textmedia.disabled = 1
      }
    }

    tx_starter_imagecols_large {
      types {
        starter_textmedia.disabled = 1
      }
    }

    tx_starter_width {
      addItems {
        container-sm = LLL:EXT:starter_freya/Resources/Private/Language/locallang_be.xlf:tx_starter_width.container-sm
        container-md = LLL:EXT:starter_freya/Resources/Private/Language/locallang_be.xlf:tx_starter_width.container-md
